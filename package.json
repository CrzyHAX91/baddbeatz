{
  "name": "baddbeatz",
  "version": "1.0.0",
  "type": "module",
  "description": "BaddBeatz DJ Portfolio Website",
  "main": "index.js",
  "scripts": {
    "deploy": "wrangler deploy",
    "pretest": "cross-env NODE_OPTIONS=--experimental-vm-modules node scripts/ensure-docs.cjs",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "build-docs": "node scripts/build-docs.cjs",
    "build": "npm run build-docs && npm run test",
    "start": "python server.py",
    "dev": "python server.py",
    "lint": "eslint assets/js/**/*.js",
    "lint:fix": "eslint assets/js/**/*.js --fix"
  },
  "keywords": [
    "dj",
    "music",
    "portfolio",
    "techno",
    "hardstyle",
    "baddbeatz"
  ],
  "author": "TheBadGuyHimself",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "wrangler": "^4.20.0"
  }
}
